<?xml version="1.0"?>
<Container version="2">
  <Name>TrackMania-Server</Name>
  <Repository>evotm/trackmania:latest</Repository>
  <Registry>https://hub.docker.com/r/evotm/trackmania</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://github.com/EvoTM/docker-trackmania</Project>
  <Overview/>
  <Category>GameServers: Status:Stable</Category>
  <WebUI/>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/L4stIdi0t/Unraid-template/master/TrackMania-Server/logoTM.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1637603683</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>2350</HostPort>
        <ContainerPort>2350</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>2350</HostPort>
        <ContainerPort>2350</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
      <Port>
        <HostPort>5005</HostPort>
        <ContainerPort>5005</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/TrackmaniaServer/</HostDir>
      <ContainerDir>/server/UserData</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value></Value>
      <Name>MASTER_LOGIN</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>MASTER_PASSWORD</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>false</Value>
      <Name>XMLRPC_ALLOWREMOTE</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>FORCE_IP_ADDRESS</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="MASTER_LOGIN" Target="MASTER_LOGIN" Default="" Mode="" Description="Container Variable: MASTER_LOGIN" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="MASTER_PASSWORD" Target="MASTER_PASSWORD" Default="" Mode="" Description="Container Variable: MASTER_PASSWORD" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="XMLRPC_ALLOWREMOTE" Target="XMLRPC_ALLOWREMOTE" Default="false" Mode="" Description="Container Variable: XMLRPC_ALLOWREMOTE" Type="Variable" Display="always" Required="true" Mask="false">false</Config>
  <Config Name="FORCE_IP_ADDRESS" Target="FORCE_IP_ADDRESS" Default="" Mode="" Description="Container Variable: FORCE_IP_ADDRESS" Type="Variable" Display="always" Required="false" Mask="false"></Config>
  <Config Name="ServerData" Target="/server/UserData" Default="/mnt/user/appdata/TrackmaniaServer/" Mode="rw" Description="Container Path: /server/UserData" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/TrackmaniaServer/</Config>
  <Config Name="Game TCP" Target="2350" Default="2350" Mode="tcp" Description="Container Port: 2350" Type="Port" Display="always" Required="true" Mask="false">2350</Config>
  <Config Name="Game UDP" Target="2350" Default="2350" Mode="udp" Description="Container Port: 2350" Type="Port" Display="always" Required="true" Mask="false">2350</Config>
  <Config Name="XMLRPC" Target="5005" Default="5005" Mode="tcp" Description="Container Port: 5005" Type="Port" Display="always" Required="false" Mask="false">5005</Config>
</Container>
